<h1>New User</h1>

<%= form_for @user do |f| %>

    <div class="field">
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div class="field">
        <%= f.label :email %>
        <%= f.text_field :email %>
    </div>

    <div class="field">
        <%= f.label :age %>
        <%= f.text_field :age %>
    </div>

    <div class="field">
        <%= f.label :bio %>
        <%= f.text_field :bio %>
    </div>

    <div class="actions">
        <%= f.submit %>
    </div>

    

<% end %>